<?import javafx.scene.layout.AnchorPane?>

<Scene xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="petshop.gui.Quantidade">
    <AnchorPane prefHeight="154.0" prefWidth="198.0">
      <children>
         <Spinner fx:id="quantidade" editable="true" layoutX="15.0" layoutY="60.0" />
         <Label layoutX="63.0" layoutY="36.0" text="Quantidade" />
         <Button layoutX="14.0" layoutY="112.0" mnemonicParsing="false" onAction="#confirmar" text="Confirmar" />
         <Button layoutX="110.0" layoutY="112.0" mnemonicParsing="false" onAction="#fechar" text="Cancelar" />
      </children></AnchorPane>
</Scene>
